package com.chowing;

/*
 * 000000
 * 001000
 * 020300
 * 000000
 */
    
public class TwoDimensionalArray01 {

    public static void main (String[] args) {
        int chessArr1[][] = new int[4][6];
        chessArr1[1][2] = 1;
        chessArr1[2][1] = 2;
        chessArr1[2][3] = 3;

        for (int[] row : chessArr1) {
            for (int data : row) {
                System.out.printf("%d", data);
            }
            System.out.println();
        }

    }   
}
